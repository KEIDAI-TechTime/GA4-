{
  "rewrites": [
    { "source": "/ga4-analytics/api/:path*", "destination": "/api/:path*" },
    { "source": "/ga4-analytics/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/ga4-analytics/:path*", "destination": "/:path*" },
    { "source": "/((?!api/).*)", "destination": "/index.html" }
  ]
}
